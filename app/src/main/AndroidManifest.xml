<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.arif_ginanjar.lesson5crudsqllite">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CreateBiodataActivity"
            android:label="@string/title_activity_buatbiodata"/>
        <activity android:name=".ReadBiodataActivity"
            android:label="@string/title_activity_lihatbiodata"/>
        <activity android:name=".UpdateBiodataActivity"
            android:label="@string/title_activity_updatebiodata"/>
    </application>

</manifest>